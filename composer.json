{
    "name": "intrawarez/slim-annotations",
    "description": "Annotations for the Slim framework",
    "homepage": "https://intrawarez.github.io/slim-annotations/",
    "authors": [
        {
            "name": "Maximilian Meffert",
            "email": "packagist@intrawarez.de"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
        	"": [
        		"src/",
        		"example/src/"
        	]
        },
        "files": [
        	"src/intrawarez/slimannotations/annotations/SlimAnnotation.php",
        	"src/intrawarez/slimannotations/annotations/Dependency.php",
        	"src/intrawarez/slimannotations/annotations/Group.php",
        	"src/intrawarez/slimannotations/annotations/Middlewares.php",
        	"src/intrawarez/slimannotations/annotations/Middleware.php",
        	"src/intrawarez/slimannotations/annotations/HttpMethod.php",
        	"src/intrawarez/slimannotations/annotations/GET.php",
        	"src/intrawarez/slimannotations/annotations/POST.php",
        	"src/intrawarez/slimannotations/annotations/PUT.php",
        	"src/intrawarez/slimannotations/annotations/DELETE.php",
        	"src/intrawarez/slimannotations/annotations/OPTIONS.php",
        	"src/intrawarez/slimannotations/annotations/POST.php",
        	"src/intrawarez/slimannotations/annotations/ANY.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require-dev": {
    
        "phpunit/phpunit": "5.5.*",
        "squizlabs/php_codesniffer": "^2.6",
		"phpmd/phpmd": "^2.4",
		"phpmetrics/phpmetrics": "^1.10"
		
    },
    "require": {
    	
        "php": ">=7",
        "slim/slim": "^3.1",
        "doctrine/annotations": "^1.2",
        "intrawarez/sabertooth": "^0.5.0"
        
    }
}
