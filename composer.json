{
    "name": "m038/sam",
    "description": "Slim Annotation Mapping",
    "homepage": "https://intrawarez.github.io/sam/",
    "authors": [
        {
            "name": "Maximilian Meffert",
            "email": "packagist@intrawarez.de"
        }
    ],
    "bin": [
        "sam"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
        	"intrawarez\\": [
        		"src/intrawarez/",
        		"example/src/intrawarez/"
        	]
        },
        "files": [
        	"src/intrawarez/sam/annotations/SlimAnnotation.php",
        	"src/intrawarez/sam/annotations/Dependency.php",
        	"src/intrawarez/sam/annotations/Group.php",
        	"src/intrawarez/sam/annotations/Middleware.php",
        	"src/intrawarez/sam/annotations/Action.php",
        	"src/intrawarez/sam/annotations/GET.php",
        	"src/intrawarez/sam/annotations/POST.php",
        	"src/intrawarez/sam/annotations/PUT.php",
        	"src/intrawarez/sam/annotations/DELETE.php",
        	"src/intrawarez/sam/annotations/OPTIONS.php",
        	"src/intrawarez/sam/annotations/PATCH.php",
        	"src/intrawarez/sam/annotations/ANY.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require-dev": {
        "phpunit/phpunit": "5.5.*",
        "squizlabs/php_codesniffer": "^2.6",
        "phpmd/phpmd": "^2.4",
        "phpmetrics/phpmetrics": "^1.10"
    },
    "require": {
        "php": ">=7",
        "slim/slim": "^3.5",
        "doctrine/annotations": "^1.2",
        "intrawarez/sabertooth": "^0.5.0",
        "symfony/finder": "^3.1",
        "nikic/php-parser": "^2.1",
        "symfony/console": "^3.1"

    }
}
